build: 
    sudo docker build -t netem-test:latest ./
run:
    sudo docker run --privileged --name netem-test -p 4433:443/udp -p 4434:4434/udp -p 4435:4435/udp --volume=/home/sven/Documents/Course_BaPr/repo/quicker/scripts/netem-tests/logs:/logs -d netem-test:latest
logs:
    sudo docker logs -f netem-test
bash:
    sudo docker exec -it netem-test bash
run client:
    node /quicker/out/mainclient.js 127.0.0.1 4433
stop:
    sudo docker stop netem-test && sudo docker rm netem-test